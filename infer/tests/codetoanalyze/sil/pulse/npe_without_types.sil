.source_language = "java"

type cell = { value:int; next: *cell }

type list = { header: *cell }

define .static without_types.basic_bad(l: *list) : void {
  #entry:
    store &l <- null
    n0 = load &l
    n1 = load n0.list.header
    ret null
}

define .static without_types.if_test_lt_ok(l: *list) : void {
  #entry:
    n4 = 2
    n5 = __sil_lt(n4, 3)
    jmp then_node, else_node
  #then_node:
    prune n5
    ret null
  #else_node:
    prune ! n5
    store &l <- null
    n0 = load &l
    n1 = load n0.list.header
    ret null
}

define .static without_types.if_test_lt_bad(l: *list) : void {
  #entry:
    n4 = 2
    n5 = __sil_lt(n4, 3)
    jmp then_node, else_node
  #then_node:
    prune n5
    store &l <- null
    n0 = load &l
    n1 = load n0.list.header
    ret null
  #else_node:
    prune ! n5
    ret null
}

define .static without_types.if_test_eq_ok(l: *list) : void {
  #entry:
    n4 = 2
    n5 = __sil_eq(n4, 2)
    jmp then_node, else_node
  #then_node:
    prune n5
    ret null
  #else_node:
    prune ! n5
    store &l <- null
    n0 = load &l
    n1 = load n0.list.header
    ret null
}

define .static without_types.if_test_eq_bad(l: *list) : void {
  #entry:
    n4 = 2
    n5 = __sil_eq(n4, 2)
    jmp then_node, else_node
  #then_node:
    prune n5
    store &l <- null
    n0 = load &l
    n1 = load n0.list.header
    ret null
  #else_node:
    prune ! n5
    ret null
}

define .static without_types.conditional_latent(n: int) : void {
  local l: *list
  #entry:
    n4 = load &n
    n5 = __sil_eq(n4, 2)
    jmp then_node, else_node
  #then_node:
    prune n5
    store &l <- null
    n0 = load &l
    n1 = load n0.list.header
    ret null
  #else_node:
    prune ! n5
    ret null
}

define .static without_types.call_ok() : void {
  #entry:
    n1 = without_types.conditional_latent(1)
    ret null
}

define .static without_types.call_bad() : void {
  #entry:
    n1 = without_types.conditional_latent(2)
    ret null
}

define .static without_types.store_bad(l1: *list, l2: *list) : void {
  #entry:
    n0 = load &l1
    store n0.list.header <- null
    n1 = load n0.list.header
    // n1 == null
    n2 = load &l2
    store n2.list.header <- n1
    n3 = load n2.list.header
    // n3 == null
    n4 = load n3.cell.value
    ret null
}

define .static without_types.array_bad(a: (*cell)[]) : void {
  #entry:
    n0 = __sil_allocate(<cell>)
    store &a[0] <- null
    store &a[1] <- n0
    n1 = load &a[0]
    n2 = load n1.cell.value
    ret null
}

define .static without_types.array_ok(a: (*cell)[]) : void {
  #entry:
    n0 = __sil_allocate(<cell>)
    store &a[0] <- null
    store &a[1] <- n0
    n1 = load &a[1]
    n2 = load n1.cell.value
    ret null
}

define .static without_types.return_null_ok(): *cell {
  #entry:
    ret null
}

define .static without_types.return_cell_ok(): *cell {
  #entry:
    n0 = __sil_allocate(<cell>)
    ret n0
}

define .static without_types.call_and_npe_bad() : void {
  #entry:
    n0 = without_types.return_null_ok()
    n1 = load n0.cell.value
    ret null
}

define .static without_types.call_and_no_npe_ok() : void {
  #entry:
    n0 = without_types.return_cell_ok()
    n1 = load n0.cell.value
    ret null
}

define .static without_types.load_bad() : void {
  local locvar: *cell
  #entry:
    store &locvar <- null
    n0 = load &locvar
