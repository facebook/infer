define return_null_ok(): *cell {
  #entry:
    ret null
}

define return_cell_ok(): *cell {
  #entry:
    n0 = __sil_allocate(<cell>)
    ret n0
}

define call_and_npe_bad() : void {
  #entry:
    n0 = return_null_ok()
    n1: int = load n0.cell.value
