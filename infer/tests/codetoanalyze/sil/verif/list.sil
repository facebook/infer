.source_language = "java"

type cell = { value:int; next: *cell }

type list = { header: *cell }

define head(l: *list) : int {
  #entry:
    n0: *list = load &l
    n1: *cell = load n0.list.header
    n2: int = load n1.cell.value
    ret n2
}
