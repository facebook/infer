global I : int

type cell = { val: int; next: *cell }

define f(x: int, y: float, n: *cell) : void {
  #entry:
    x: int = load y
    x: int = load n[1]
    jmp lab1
  #lab1:
    store x <- y:int
    store x[1] <- y:int
    x = f()
    ret x

}
