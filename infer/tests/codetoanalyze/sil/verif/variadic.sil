.source_language = "hack"

define f1(x: int, y: int, args: .variadic mixed) : int {
  #entry:
    ret f2(x)
}

define f2(x: int, y: int, args: .variadic mixed) : int {
  #entry:
    ret f1(x, y, 0, 1)
}

define f3(x: int, y: int, args: .variadic mixed, z: int) : int {
  #entry:
    ret f1(x, y, z)
}
