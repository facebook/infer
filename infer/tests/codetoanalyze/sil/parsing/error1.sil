global I

type node = { val: int; next: *node }

define f(x: int, y: float, n: *node) : void {
  #entry:
    x: int = load y
    x: int = load n[1]
    jmp lab1
  #lab1:
    store x <- y:int
    store x[1] <- y:int
    x = f()
    ret x

}
