in Java match
    {
    void  fun6tt(A[] a)  throws A;
    A[][]  methD66(A.B[] x)  throws D
           under classFF
    public static void  madMeth(A[] a)  throws A
           under A.B.D<E,G>
    A[]  meth4Aa( )  throws B.E;
    void  funSD(A[] a)  throws B.E
           under classFF
    void  fun6tt(A[] a)  throws A under classC
     }
body
    { bodyStub }

