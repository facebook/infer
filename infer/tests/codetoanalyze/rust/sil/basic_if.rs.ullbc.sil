.source_language = "Rust"

define basic_if::main() : void {
  local var_0: void, b_1: int, var_2: int
  #node_0:
      store &b_1 <- 1:int
      n0:int = load &b_1
      store &var_2 <- n0:int
      n1:int = load &var_2
      jmp node_1, node_2
      
  #node_1:
      prune n1
      store &b_1 <- 0:int
      store &var_0 <- null:void
      jmp node_3
      
  #node_2:
      prune __sil_lnot(n1)
      store &b_1 <- 1:int
      store &var_0 <- null:void
      jmp node_3
      
  #node_3:
      store &var_0 <- null:void
      n2:void = load &var_0
      ret n2
      
}

